{
  "name": "netflix-app",
  "version": "1.0.0",
  "description": "netflix app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --node-env=development --open ",
    "build": "webpack --node-env=production",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install",
    "test": "jest"
  },
  "jestWebpackResolver": {
    "webpackConfig": "./webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volbo/netflix-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/volbo/netflix-app/issues"
  },
  "homepage": "https://github.com/volbo/netflix-app#readme",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.5.0",
    "redux": "^4.2.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.5",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jest-webpack-resolver": "^0.3.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write"
    ],
    "src/**/*.jsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.tsx": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ],
    "resolver": "jest-webpack-resolver"
  }
}
